    "url": "https://github.com/CarpeSwag/Team-Nahual-Website/issues"
  },
  "homepage": "https://github.com/CarpeSwag/Team-Nahual-Website#readme",
  "dependencies": {
    "inferno": "^3.9.0",
    "inferno-component": "^3.9.0"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-inferno": "^3.2.0",
    "babel-preset-es2015": "^6.24.1",
    "css-loader": "^0.28.7",
    "html-minifier": "^3.5.5",
    "html-minifier-loader": "^1.4.0",
    "html-webpack-plugin": "^2.30.1",
