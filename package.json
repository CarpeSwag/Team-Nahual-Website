{
  "name": "teamnahualwebsite",
  "version": "1.0.0",
  "description": "Team Nahual Website",
  "main": "index.js",
  "scripts": {
    "build": "npm run build-sass && npm run webpack && npm run copy-res",
    "build-production": "npm run build-sass && webpack && npm run copy-res",
    "webpack": "webpack --config webpack.config.dev.js",
    "build-sass": "node-sass --include-path scss ./src/index.scss ./build/css/index.css --output-style compressed",
    "copy-res": "cp -a ./src/res/. ./build/res/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/CarpeSwag/Team-Nahual-Website.git"
  },
